{
  "name": "ripplecheck",
  "displayName": "RippleCheck",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ripplecheck-sidebar",
          "title": "RippleCheck",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "ripplecheck-sidebar": [
        {
          "type": "webview",
          "id": "ripplecheck.gitVisualizer",
          "name": "Git Visualizer",
          "icon": "media/icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "ripplecheck.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "cytoscape": "^3.33.1",
    "simple-git": "^3.31.1",
    "ts-morph": "^27.0.2"
  }
}